/**
 * Returns a cursor with all log messages.
 */
create function stratum_log_fetch()
returns refcursor
as $$
declare
  cur refcursor;
-- type: hidden
begin
  open cur
  for
  select tmp_time
  ,      tmp_message
  from   tmp_stratum_log
  order by tmp_log
  ;

  return cur;
end;
$$ language plpgsql;
